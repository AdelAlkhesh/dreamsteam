<div class="wholePage">
  <div class="pagetitle">
    <h1>Games Owned: {{count}}</h1>
  </div>

  <div class="ownedGamesContainer">
    {{#each list}}
      <div class="stats">
        <img
          loading="lazy"
          src="https://steamcdn-a.akamaihd.net/steam/apps/{{this.appid}}/header.jpg"
          alt="Not available"
        />
        <h3>{{this.name}}</h3>
        <h5>Playtime: {{this.playtime_forever}} hours</h5>
      </div>
    {{/each}}
  </div>
</div>